{
  "root": true,
  "parser": "@babel/eslint-parser",
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "prettier"
  ],
  "rules": {
    "no-undef": "warn",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
